{
  "name": "class-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "npm run build:css && remix dev",
    "start": "remix-serve build/index.js",
    "typecheck": "tsc",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{ts,tsx}' --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "format:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
   "prepare": "echo 'Skipping husky setup'",
    "test": "vitest run --coverage",
    "lint-staged": "lint-staged",
    "build:css": "sass ./app/index.scss ./app/index.css"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.11.1",
